<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yongf.rorderwaiter">

    <!--permissions-->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".app.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".app.activity.LoginActivity" />
        <activity android:name=".app.activity.AboutActivity" />
        <activity android:name=".app.activity.AccountActivity" />
        <activity android:name=".app.activity.MyOrderActivity" />
        <activity android:name=".app.activity.OrderDetailActivity" />
    </application>

</manifest>
